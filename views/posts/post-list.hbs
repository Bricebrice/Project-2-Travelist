
{{#if post.length}}
{{#each post}}
<ul>
    <li>
        <strong>
            {{this.name}}
        </strong>
    </li>
    <li>
        {{this.location}}
    </li>
    <li>
        {{this.duration}} days
    </li>
    <li>
        {{this.distance}} km
    </li>
    <li>
        {{this.typeOfTrip}}
    </li>
    <li>
        {{this.itinerary}}
    </li>
</ul>
{{/each}}

{{else}}
{{/if}}


<a href="/explore/posts/create"> Create new post</a>